{
  "include": ["./src/front/gatsby/index.ts"],
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "lib": ["dom", "esnext"],
    "allowJs": true,
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "outDir": "./gatsby-dist",
    "baseUrl": "./",

  },
  "exclude": ["node_modules", "public", ".cache"]
}
